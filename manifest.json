{
  "name": "LeetRepeet",
  "version": "1.0.1",
  "description": "Spaced Repetition for better problem retention and memory on leetcode.com",
  "permissions": [
    "storage",
    "declarativeContent",
    "tabs",
    "https://leetcode.com/",
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "16": "images/16x16dark.png",
    "32": "images/32x32dark.png",
    "48": "images/48x48dark.png",
    "128": "images/128x128dark.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/16x16dark.png",
      "32": "images/32x32dark.png",
      "48": "images/48x48dark.png",
      "128": "images/128x128dark.png"
    }
  },
  "content_security_policy": "script-src 'self' https://cdn.jsdelivr.net/alasql/0.2/alasql.min.js 'unsafe-eval'; object-src 'self'",
  "manifest_version": 2
}
